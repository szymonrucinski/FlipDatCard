{
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/proposal-object-rest-spread",
      [
        "@babel/proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "@babel/proposal-class-properties",
        {
          "loose": true
        }
      ]
    ]
  }